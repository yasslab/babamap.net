---
---

[
  {% for item in site.data.takadanobaba %}
    {
      "id":    "{{ item[':id'] }}",
      "src":   "{{ item[':src'] }}",
      "lat":   "{{ item[':lat'] }}",
      "lng":   "{{ item[':lng'] }}",
      "link":  "{{ item[':link'] }}",
      "date":  "{{ item[':date'] }}",
      "image": "{{ item[':image'] }}",
      "title": "{{ item[':title'] | xml_escape }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
