---
---

{% assign markers = site.data.takadanobaba | where_exp: "marker", "marker[':title'] != '404_Not_Found'" %}

[
  {% for marker in markers %}
    {
      "id":    "{{ marker[':id'] }}",
      "src":   "{{ marker[':src'] }}",
      "lat":   "{{ marker[':lat'] }}",
      "lng":   "{{ marker[':lng'] }}",
      "link":  "{{ marker[':link'] }}",
      "date":  "{{ marker[':date'] }}",
      "image": "{{ marker[':image'] }}",
      "title": {{ marker[':title'] | jsonify }}
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]

